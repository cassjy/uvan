.name{
	box-sizing: border-box;
	width: 100%;
	height: 247rpx;
	font-size: 0;
	padding-top: 75rpx;
	text-align: center;
}
.name .nameImg{
	width: 240rpx;
}
.product{
	width: 100%;
	font-size: 0;
}
.product .productImg{
	width: 100%;
}
.tab{
	box-sizing: border-box;
	width: 100%;
	height: 131rpx;
	font-size: 30rpx;
	color: #666;
}
.tab1,.tab2{
	box-sizing: border-box;
	float: left;
	padding-top: 40rpx;
	width: 50%;
	height: 100%;
	text-align: center;
}
.tab .tabname{
	box-sizing: border-box;
	margin: 0 auto;
	width: 120rpx;
	height: 60rpx;
	line-height: 60rpx;
}
.tab .tabborder{
	color: #000;
	border-bottom: 1px solid #000;
}
.tabcontent{
	width: 100%;
	font-size: 0;
}
.tabcontent .content{
	width: 100%;
}
.factory{
	padding-top: 45rpx;
	font-size: 0;
}
.factory .title{
	width: 100%;
	margin-bottom: 46rpx;
}
.factory .content{
	width: 100%;
}
.more{
	box-sizing: border-box;
	font-size: 0;
	padding-top: 60rpx;
	padding-bottom: 80rpx;
}
.more .btn{
	width: 100%;
	height: 106rpx;
}