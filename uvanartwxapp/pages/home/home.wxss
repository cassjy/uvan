}

/* ::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}  */

@keyframes fadeIn {
  from {
    opacity: 0;
  }

  to {
    opacity: 1;
  }
}

.fadeIn {
  animation-name: fadeIn;
  animation-duration: 0.3s;
}

::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}

