.shareWin {
  width: 100%;
  height: 320rpx;
  display: flex;
  position: fixed;
  bottom: -400rpx;
  left: 0;
  background-color: #fff;
  z-index: 1002;
  transition: bottom 0.5s;
}

.sharewin1 {
  bottom: 0rpx;
}

.shareWin .share-link {
  flex: 1;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

.shareWin .share-link .iconfont {
  width: 128rpx;
  height: 128rpx;
  border: 1px solid #bfbfbf;
  border-radius: 50px;
  font-size: 54rpx;
  color: #222;
  display: flex;
  justify-content: center;
  align-items: center;
