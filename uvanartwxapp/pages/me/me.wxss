/* 弹窗样式 */

.shade {
  position: fixed;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  overflow: hidden;
  outline: 0;
  background-color: rgba(0, 0, 0, 0.2);
  z-index: 10;
  color: white;
}

.box {
  position: fixed;
  z-index: 50;
  width: 600rpx;
  height: 600rpx;
  top: 50%;
  left: 50%;
  -webkit-transform: translate(-50%, -50%);
  transform: translate(-50%, -50%);
  background-color: #fff;
  text-align: center;
  border-radius: 5px;
  overflow: hidden;
}

.git {
  width: 275rpx;
  height: 220rpx;
  margin: 120rpx auto;
}

.git-dec {
  font-size: 28rpx;
  color: #aaa;
}

/*遮罩层*/

.cover {
  height: 100%;
  width: 100%;
  background-color: rgba(0, 0, 0, 0.5);
  position: fixed;
  top: 0;
  left: 0;
  z-index: 1001;
}

