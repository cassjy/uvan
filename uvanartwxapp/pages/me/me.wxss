  justify-content: center;
  align-items: center;
  margin-bottom: 19rpx;
}

.stars-icon>view .face {
  font-size: 40rpx;
}

.stars-icon>view .ablumn {
  font-size: 40rpx;
}

.stars-icon>view .smile {
  font-size: 46rpx;
}

.stars-icon>view>button {
  position: absolute;
  height: 108rpx;
  width: 120rpx;
  opacity: 0;
}

.stars-icon>view>text, .stars-icon>navigator>text {
