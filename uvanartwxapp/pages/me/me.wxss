/*引入字体样式  */

@font-face {
  font-family: myApple;
  src: url('https://uvpt.uvanart.com/upload/static/yingdaotu/jiantou.png');
}

page {
  width: 100%;
  height: 100%;
  background: #f4f4f4;
}

.top {
  height: 107rpx;
}

/* 头部导航 */

.top-banner {
  height: 107rpx;
  line-height: 107rpx;
  display: flex;
  position: fixed;
  top: 0;
  width: 100%;
  z-index: 1000;
  justify-content: space-around;
  background: #fff;
  font-size: 34rpx;
  color: #424242;
  border-top: 1rpx solid #f2f2f2;
  /* box-shadow: 0 0 20rpx rgba(0, 0, 0, 0.2); */
}

.topbanner {
  flex: 1;
  display: flex;
  justify-content: center;
  align-items: center;
  height: 100%;
}

.top-banner .title {
  color: #888;
  width: 109rpx;
  height: 100%;
  text-align: center;
  box-sizing: border-box;
}

.top-banner .checked {
  color: #222;
  border-bottom: 4rpx solid #222;
}

.top-banner .bottom-line {
  width: 0;
  height: 8rpx;
  margin: 0 auto;
  background: #e8364c;
  transition: all 0.5s ease;
}

/* .show-banner {
  width: 60rpx !important;
} */

/* 梵星 */

.stars {
  margin-top: 20rpx;
  transition: all 0.5s ease;
}

/* 头部卡片 */

.stars .stars-top {
  background: #fff;
  /* border-radius: 16rpx;
  box-shadow: 0 0 20rpx rgba(0, 0, 0, 0.2); */
  color: #424242;
  padding: 40rpx;
  box-sizing: border-box;
}

.stars .stars-top .list-1 {
  margin-bottom: 31rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.stars .stars-top .list-1 .person-data {
  display: flex;
  flex-direction: row;
  align-items: center;
  flex: 1;
}

.stars .stars-top .list-1 .person-data image {
  width: 90rpx;
  height: 90rpx;
  border-radius: 50%;
  margin-right: 20rpx;
  /* border: 1px solid #eee;  */
}

.stars .stars-top .list-1 .person-data>view {
  width: 300rpx;
  padding-top: 10rpx;
  box-sizing: border-box;
  /* display: flex;
  flex-wrap: wrap;
  align-content: space-between; */
}

.stars .stars-top .list-1 .person-data .nickname {
  color: #424242;
  font-size: 30rpx;
  font-weight: bold;
  width: 300rpx;
  line-height: 1;
  margin-top: 2rpx;
}

.stars .stars-top .list-1 .person-data .date {
  font-size: 22rpx;
  color: #c7c7c7;
  /* background: #f1f1f1; */
  border-radius: 16rpx;
  height: 38rpx;
  line-height: 38rpx;
  width: 280rpx;
  margin-top: 18rpx;
  letter-spacing: 2rpx;
  /* text-align: center; */
}

.stars .stars-top .list-1 .right {
  font-size: 28rpx;
  color: #fff;
  width: 98rpx;
  height: 98rpx;
  line-height: 98rpx;
  text-align: center;
  border-radius: 100%;
  /* border: 1rpx solid #ececec; */
  background-color: #424242;
  /* display: flex;
  align-items: center;
  justify-content: center; */
}

.stars .stars-top .list-1 .right image {
  width: 30rpx;
  height: 30rpx;
  margin-right: 10rpx;
}

/*高亮  */

.hightLight {
  background-color: #ececec;
  background: linear-gradient(to bottom right, rgba(236, 236, 236, 1), rgba(243, 243, 243, 0.9));
}

/* changecolor */

.changecolor {
  background-color: #aaa;
  background: linear-gradient(to bottom right, rgba(164, 165, 168, 1), rgba(220, 220, 220, 0.9));
}

.stars .stars-top .list-2 {
  height: 88rpx;
  color: #424242;
  border-bottom: 1px solid #dedede;
  /* border-top: 1px solid #dedede; */
  font-size: 32rpx;
  line-height: 88rpx;
}

.stars .stars-top .list-2 view {
  font-size: 28rpx;
  display: inline;
}

.stars .stars-top .list-2 navigator {
  font-size: 22rpx;
  color: #888;
  float: right;
  display: flex;
  align-items: center;
}

.icon-more {
  font-size: 30rpx;
}

.stars .stars-top .list-3 {
  display: flex;
  justify-content: space-between;
  margin-top: 40rpx;
  font-size: 22rpx;
  color: #424242;
}

.stars .stars-top .list-3>view {
  display: flex;
  flex-direction: column;
  align-items: center;
  width: 108rpx;
  height: 90rpx;
  box-sizing: border-box;
}

.stars .stars-top .list-3 .img {
  color: #2c2c2c;
  /* width: 40rpx;
  height: 40rpx; */
  margin-bottom: 20rpx;
  font-size: 40rpx;
}

/* .stars .stars-top .list-3 .icon-daifahuo2 {
  font-size: 42rpx;
}  */

/* 中间卡片 */

.stars-icon {
  background: #fff;
  /* border-radius: 16rpx;
  box-shadow: 0 0 20rpx rgba(0, 0, 0, 0.2); */
