/*字体图标样式  */

.icon-more {
  font-family: "iconfont" !important;
  font-size: 30rpx;
  font-style: normal;
  color: #717171;
  -webkit-font-smoothing: antialiased;
  -webkit-text-stroke-width: 0.2px;
  -moz-osx-font-smoothing: grayscale;
}

.icon-more::before {
  content: '\e6a7';
}

/*内容部分样式  */

page {
  height: 100%;
  /*background-color: #f4f4f4;*/
  position: relative;
}

.setting {
  font-size: 28rpx;
  margin-top: 18rpx;
  background-color: #fff;
}
.clearCache,.version{
  margin-left: 40rpx;
  margin-right: 40rpx;
  border-bottom: 1px solid #dedede;
  height: 108rpx;
  line-height: 108rpx;
  color: #424242;
  font-size: 30rpx;
}

.phoneNum-win{
  box-sizing: border-box;
  height: 154rpx;
  margin-right: 40rpx;
  margin-left: 40rpx;
  border-bottom: 1px solid #dedede;
}
.phoneNum-win .win-left{
  box-sizing: border-box;
  float: left;
  height: 152rpx;
  padding-top: 40rpx;
}
.phoneNum-win .win-left .title1{
  font-size: 30rpx;
  color: #424242;
  margin-bottom: 10rpx;
}
.phoneNum-win .win-left .title2{
  font-size: 24rpx;
  color: #aaa;
}
.phoneNum-win .win-right{
  float: right;
  height: 152rpx;
}
.phoneNum-win .win-right .phoneNum{
  display: inline-block;
  height: 152rpx;
  line-height: 152rpx;
  margin-right: 20rpx;
  color: #424242;
}
.phoneNum-win .win-right .iconfont{
  display: inline-block;
  height: 152rpx;
  line-height: 152rpx;
  color: #222;
  font-size: 30rpx;
}

