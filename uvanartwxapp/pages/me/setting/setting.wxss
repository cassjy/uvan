.version .versionNum{
  margin-left: 40rpx;
}

/*退出登录  */
.exit-win{
  width: 100%;
  position: absolute;
  bottom: 80rpx;
}
.exit{
  box-sizing: border-box;
  width: 670rpx;
  height: 96rpx;
  border: 1px solid #dedede;
  text-align: center;
  line-height: 96rpx;
  font-size: 32rpx;
  color: #424242;
  letter-spacing: 4rpx;
  margin: 0 auto;
}

/*未开发提示  */
.tips{
  opacity: 0;
  position: fixed;
  top: 50%;
  width: 100%;
  text-align: center;
}
.tips .tipsWrapper{
  color: #fff;
  background-color: rgba(0,0,0,0.6);
  border-radius: 6rpx;
  font-size: 28rpx;
  padding: 13rpx 50rpx;
}