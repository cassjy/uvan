page{
  border-top: 1px solid #f2f2f2;
}
.scroll{
  height:99.8vh;
  background: #f8f8f8;
}
.header{
  height: 230rpx;
  background: #fff;
  padding: 40rpx 50rpx;
  box-sizing:border-box;
}
.header .header-img{
  display: inline-block;
  width: 130rpx;
  height: 130rpx;
  margin-right: 30rpx;
  background: #e6e6e6;
  border-radius: 100%;
  float:left;
}
.header .header-img .icon-xiangji2{
  font-size: 56rpx;
  margin: 35rpx 31.5rpx;
  color: #fff;
  display: inline-block;
}
.header .header-img .head{
  width: 130rpx;
  height: 130rpx;
  border-radius: 100%;
}
.header .header-text{
  font-size: 36rpx;
  color: #424242;
  display: inline-block;
  height: 130rpx;
  line-height: 130rpx;
}
.header .header-msg{
  display: inline-block;
  float:left;
  letter-spacing:0.5px;
}
.header .header-msg .header-name{
  font-size:36rpx;
  color:#424242;
  height:90rpx;
  line-height:90rpx;
}
.header .header-msg .header-ng{
  color: #888888;
  font-size: 24rpx;
}
.header .header-msg .header-ng .icon-xiangji2{
  color: #484848;
  font-size: 24rpx;
}
.title-top{
  position:fixed;
  top:0;
  width:100%;
  z-index:100;
}
.title-height{
  height:110rpx;
  width: 100%;
}
.nav{
  height:130rpx;
  display: flex;
  justify-content: space-around;
  background: #fff;
  transition: all .3;
  z-index: 8;
  z-index:100;
}
.nav-list{
  margin-top: 20rpx;
  height: 90rpx;
  line-height: 90rpx;
  border-radius: 4rpx;
  width: 190rpx;
  text-align: center;
  color: #666666;
  font-size: 32rpx;
}
.nav-list-active{
  background: #f4f4f4;
  color: #222222;
}
.photo{
  margin: 20rpx;
  overflow: hidden;
}
.photo .item-list{
  height: 234rpx;
  width: 234rpx;
  display: inline-block;
  float: left;
  margin-bottom: 4rpx;
  position: relative;
}
.photo .item-list view{
  height: 234rpx;
  width: 234rpx;
  display: inline-block;
  z-index: 10;
  position: absolute;
  top: 0;
  left: 0;
}
.photo .item-list:nth-child(3n-1){
  margin: 0 4rpx 4rpx;
}
.icon-youfanyishulogomin{
  color:#fff;
  text-align:center;
  width:100%;
  display:inline-block;
  font-size:140rpx;
  margin-top:46rpx;
}
.no-photo{
  margin:144px auto 0;
  width:220rpx;
  text-align:center;
}
.no-photo image{
  width: 160rpx;
  height: 160rpx;
}
.edit{
  position: fixed;
  bottom: 40rpx;
  right: 20rpx;
  background: rgba(50,50,50,0.6);
  border-radius: 100%;
  text-align: center;
  height: 80rpx;
  width: 80rpx;
  line-height: 80rpx;
  color: rgba(255,255,255,0.8);
  font-size:38rpx;
  z-index: 100;
}

.item-list checkbox{
  position: absolute;
  border-radius: 100%;
  right: 0;
  top: 0;
  width: 50rpx;
  height: 50rpx;
  border:none;
  padding:6rpx 6rpx 20rpx 20rpx;
  z-index:10;
}
.item-list checkbox .wx-checkbox-input{
   width: 42rpx;
   height: 42rpx;
   border-radius: 100%;
   background: rgba(0,0,0,0.6);
   border: 4rpx solid rgba(255,255,255,0.6);
}
.item-list checkbox .wx-checkbox-input.wx-checkbox-input-checked{
   background: #e84d51;
   border: 4rpx solid #e84d51;
}
checkbox .wx-checkbox-input.wx-checkbox-input-checked::before {
  width: 42rpx;
  height:42rpx;
  line-height: 42rpx;
  text-align: center;
  font-size: 22rpx;
  color: #fff;
  background: transparent;
  transform: translate(-50%, -50%) scale(1);
  -webkit-transform: translate(-50%, -50%) scale(1);
}

.hideDelete{
  bottom: -50vh !important;
}
.delete{
  transition: all 0.3s ease;
  height: 96rpx;
  position: fixed;
  width: 710rpx;
  bottom: 0;
  background: #fff;
  display: flex;
  justify-content: space-between;
  align-items:center;
  font-size:32rpx;
  padding: 0 20rpx;
  color: #424242;
  z-index: 15;
}
.delete .btn .sure{
  text-align: center;
  color: #fff;
  height: 56rpx;
  line-height: 56rpx;
  width: 160rpx;
  border-radius:4rpx;
  background:#424242;
  display: inline-block;
}
.delete .btn .cancle{
  text-align: center;
  color: #424242;
  height: 52rpx;
  line-height: 52rpx;
  width: 156rpx;
  border-radius:4rpx;
  border: 1px solid #a0a0a0;
  background:#fff;
  display: inline-block;
  margin-right: 20rpx;
}
.delete checkbox{
  position: relative;
}
.delete checkbox-group{
  display:flex;
  justify-content:center;
  align-items:center;
  padding-left: 8rpx;
}
.delete checkbox .wx-checkbox-input{
   width: 42rpx;
   height: 42rpx;
   border-radius: 100%;
   background: #fff;
   border: 4rpx solid #a0a0a0;
}
.delete checkbox .wx-checkbox-input.wx-checkbox-input-checked{
   background: #e84d51;
   border: 3rpx solid #e84d51;
}

.download{
  margin:20rpx 40rpx 0 40rpx;
  color: #fff;
  font-size: 28rpx;
  background: #424242;
  height:80rpx;
  line-height: 80rpx;
}
.progress{
  position:fixed;
  top:40%;
  background:#fff;
  border-radius:8rpx;
  width:70%;
  z-index:100;
  text-align:center;
  height:16vh;
  left:15%;
  line-height:10vh;
  font-size:32rpx;
}
.progress progress{
  width:90%;
  margin:0 5%;
}
.progress-mode{
  position: fixed;
  background: rgba(0,0,0,0.4);
  top: 0;
  left: 0;
  height: 100vh;
  width: 100vw;
}
.progress .title{
  margin:20rpx 0;
  color:#424242;
  text-align: center;
}
.progress .progress-length{
  width:80%;
  height:40rpx;
  background:#eee;
  margin:50rpx 10%;
  border-radius:20rpx;
  overflow: hidden;
}
.progress .progress-length cover-view{
  background:#44B549;
  height: 40rpx;
  transition: all 1s;
}
.videolist{
  width: 670rpx;
  height: 376rpx;
  position: relative;
  margin: 40rpx 40rpx 0 40rpx;
  background: #000;
}
.video{
  position:fixed;
  top:33%;
  left:0;
  z-index: 10;
  transition: .3s ease-in-out;
  width: 0;
  height: 0;
  visibility: hidden;
  opacity: 0;
}
.showvideo{ 
  width: 100%;
  height: 450rpx;
  visibility: visible;
  opacity: 1;
}
.video-mode{
  width: 100%;
  height:100vh;
  position: fixed;
  top: 0;
  left: 0;
  z-index:-1;
  background: rgba(0,0,0,0);
  transition: .6s ease-in-out;
}
.show-mode{
  background: rgba(0,0,0,0.8);
  z-index:80;
}
.download-img{
  position: absolute;
  top: 0rpx;
  left: 0rpx;
  padding: 10rpx;
  width: 60rpx;
  height: 60rpx;
  z-index: 8;
}
.icon-xia1{
  color:#fff;
  position:absolute;
  font-size:60rpx;
  top:50%;
  left:50%;
  padding:60rpx;
  transform: translate3d(-50%,-50%,0) rotate(-90deg);
}
.loading{
  display:flex;
  justify-content:center;
  align-items:center;
  font-size:32rpx;
  color:#424242;
  margin:20rpx;
  transition:all .3s ease-in-out;
  visibility: hidden;
  opacity: 0;
}
.loading image{
  width:40rpx;
  height:40rpx;
}
.showloaing{
  visibility: visible;
  opacity: 1;
}