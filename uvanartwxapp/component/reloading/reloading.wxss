/* component/reloading/reloading.wxss */
@font-face {
  font-family: 'iconfont';  /* project id 515371 */
  src: url('//at.alicdn.com/t/font_515371_zvrlwocj3ndygb9.eot');
  src: url('//at.alicdn.com/t/font_515371_zvrlwocj3ndygb9.eot?#iefix') format('embedded-opentype'),
  url('//at.alicdn.com/t/font_515371_zvrlwocj3ndygb9.woff') format('woff'),
  url('//at.alicdn.com/t/font_515371_zvrlwocj3ndygb9.ttf') format('truetype'),
  url('//at.alicdn.com/t/font_515371_zvrlwocj3ndygb9.svg#iconfont') format('svg');
}
.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}
.icon-check:before { content: "\e601"; }
.icon-ioscloseoutline:before { content: "\e634"; }
.icon-warning:before{content:"\e659";}
.shade{
   position: fixed;
    top: 0;
    right: 0;
    bottom: 0;
    left: 0;
    overflow: hidden;
    outline: 0; 
    background-color: rgba(0, 0, 0, 0.0); 
    z-index: 10;
    color: white;
}
view{
  overflow: visible;
}
.box{
    position: fixed;
    z-index: 50;
    width: 80%;
    max-width: 200rpx;
    max-height: 300rpx;
    top: 50%;
    left: 50%;
    -webkit-transform: translate(-50%, -50%);
    transform: translate(-50%, -50%);
    background-color: rgba(0, 0, 0, 0.7);
    text-align: center;
    border-radius: 3px;
    overflow: hidden;
}
.loading{
  position:relative;
  width:200rpx;
  height: 200rpx;
  margin-top:-50rpx; 
}
.success{
  position:relative;
  width:200rpx;
  height: 150rpx;
}
.iconfont1{
  font-size: 150rpx;
  color: white;
}
@keyframes loading {
            from {opacity: 1;width:8rpx;height:20rpx;}
            to {opacity: 0.25;width:8rpx;height:20rpx;}
        }
         .spinner {
            position: absolute;
            top:50%;
            left:50%;
            display: inline-block;
        }
         .spinner1{
            position: absolute;
            top:53%;
            left:49%;
            display: inline-block;
        }
         .spinner>.bar {
           position: absolute;
            width:3rpx;
            height:20rpx;
            background: #fff;
            left: 100%;
            top: 100%;
            border-radius: 1rpx;
        }
         .spinner>.bar8{
            opacity: 0;
        }

         .spinner>.bar1 {transform:rotate(0deg) translate(0, -19rpx); }
         .spinner>.bar2 {transform:rotate(30deg) translate(0, -19rpx); }
         .spinner>.bar3 {transform:rotate(60deg) translate(0, -19rpx); }
         .spinner>.bar4 {transform:rotate(90deg) translate(0, -19rpx); }
         .spinner>.bar5 {transform:rotate(120deg) translate(0, -19rpx); }
         .spinner>.bar6 {transform:rotate(150deg) translate(0, -19rpx); }
         .spinner>.bar7 {transform:rotate(180deg) translate(0, -19rpx); }
         .spinner>.bar8 {transform:rotate(210deg) translate(0, -19rpx); }
         .spinner>.bar9 {transform:rotate(240deg) translate(0, -19rpx); }
         .spinner>.bar10 {transform:rotate(270deg) translate(0, -19rpx); }
         .spinner>.bar11 {transform:rotate(300deg) translate(0, -19rpx); }
         .spinner>.bar12 {transform:rotate(330deg) translate(0, -19rpx); }
         .spinner>.circle{
            position: absolute;
            transform:translate(-8rpx, 0rpx);
            width: 20rpx;
            height: 20rpx;
            border-radius: 50%;
            background-color: #fff;

        }
         .spinner>.rect{
            position: absolute;
            transform:translate(-17rpx, 0rpx);
            width: 70rpx;
            height: 110rpx;
            border-radius: 50%;
            background-color: rgba(255,255,255,0);
            border:5rpx #fff solid;
            border-color:transparent transparent transparent #fff;
        }
        .spinner1>.bar  {
            position: absolute;
            width:8rpx;
            height:8rpx;
            background: #fff;
            left: 50%;
            top: 50%;
            opacity: 0;
            border-radius: 50%;
            animation: loadingcicle 2s linear infinite;
        }
         .spinner1>.bar1 {transform:rotate(0deg) translate(0, -33rpx); animation-delay: -0s;}
         .spinner1>.bar2 {transform:rotate(30deg) translate(0, -33rpx); animation-delay: -1.8334s;}
         .spinner1>.bar3 {transform:rotate(60deg) translate(0, -33rpx); animation-delay: -1.6668s;}
         .spinner1>.bar4 {transform:rotate(90deg) translate(0, -33rpx); animation-delay: -1.5002s;}
         .spinner1>.bar5 {transform:rotate(120deg) translate(0, -33rpx); animation-delay: -1.3336s;}
         .spinner1>.bar6 {transform:rotate(150deg) translate(0, -33rpx); animation-delay: -1.167s;}
         .spinner1>.bar7 {transform:rotate(180deg) translate(0, -33rpx); animation-delay: -1.0004s;}
         .spinner1>.bar8 {transform:rotate(210deg) translate(0, -33rpx); animation-delay: -0.8338s;}
         .spinner1>.bar9 {transform:rotate(240deg) translate(0, -33rpx); animation-delay: -0.6672s;}
         .spinner1>.bar10 {transform:rotate(270deg) translate(0, -33rpx); animation-delay: -0.5006s;}
         .spinner1>.bar11 {transform:rotate(300deg) translate(0, -33rpx); animation-delay: -0.334s;}
         .spinner1>.bar12 {transform:rotate(330deg) translate(0, -33rpx); animation-delay: -0.1674s;}
         .spinner1>.bar8{
            animation:none;
        }
        @keyframes loadingcicle {
            from {opacity: 1;}
            to {opacity: 0.1;}
        }